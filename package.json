{
  "name": "react-portfolio",
  "version": "1.0.0",
  "description": "My portfolio shows recent projects I've designed and developed in the Coding Program at UNC Charlotte.",
  "dependencies": {
    "bootstrap": "^5.0.2",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-beta.4",
    "react-download-link": "^2.3.0",
    "react-dom": "^17.0.2",
    "react-pdf": "^5.3.2",
    "react-responsive-carousel": "^3.2.20",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "reactstrap": "^8.9.0",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "concurrently": "^6.2.0"
  },
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build"
